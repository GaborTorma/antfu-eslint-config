{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "3.0.0",
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "stub": "tsup",
    "dev": "pnpm stub --watch & pnpm dlx @eslint/config-inspector",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prerelease": "pnpm build && pnpm test:types && pnpm lint",
    "release": "dotenv release-it",
    "test:types": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.3.0",
    "defu": "^6.1.4",
    "eslint": "9.21.0",
    "eslint-plugin-format": "1.0.1",
    "prettier": "3.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "dotenv-cli": "^8.0.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.4.1",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.6",
    "typescript": "5.6.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
