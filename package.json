{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "1.5.3",
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & npx @eslint/config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stub:lint": "nr stub && eslint .",
    "stub:lint:fix": "nr stub && eslint . --fix",
    "release": "nr build && bumpp && npm publish --access=public",
    "postrelease": "pnpm update \"@gabortorma/antfu-eslint-config\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.7.1",
    "@nuxt/eslint": "0.5.7",
    "defu": "^6.1.4",
    "eslint": "9.10.0",
    "eslint-plugin-format": "0.1.2",
    "prettier": "3.3.3",
    "vite-plugin-eslint2": "4.4.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@eslint/config-inspector": "^0.5.4",
    "@types/node": "^22.5.5",
    "bumpp": "^9.5.2",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.11.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vitest": "^2.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
