{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "3.0.1",
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "antfu"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "stub": "tsup",
    "dev": "pnpm stub --watch & pnpm dlx @eslint/config-inspector",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prerelease": "pnpm build && pnpm test:types && pnpm lint",
    "release": "dotenv release-it",
    "test:types": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.13.0",
    "defu": "^6.1.4",
    "eslint": "9.26.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-plugin-format": "1.0.1",
    "prettier": "3.5.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "dotenv-cli": "^8.0.0",
    "lint-staged": "^16.0.0",
    "pnpm": "^10.10.0",
    "release-it": "^19.0.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
