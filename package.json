{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "3.2.0",
  "packageManager": "pnpm@10.13.1",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "antfu"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "stub": "tsup",
    "dev": "pnpm stub --watch & pnpm dlx @eslint/config-inspector",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prerelease": "pnpm build && pnpm test:types && pnpm lint",
    "release": "dotenv release-it",
    "test:types": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:",
    "defu": "catalog:",
    "eslint": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "eslint-plugin-format": "catalog:",
    "prettier": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "lint-staged": "catalog:",
    "pnpm": "catalog:",
    "release-it": "catalog:",
    "release-it-pnpm": "catalog:",
    "rimraf": "catalog:",
    "simple-git-hooks": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
