{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "2.1.0",
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "pnpm stub --watch & pnpm dlx @eslint/config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prerelease": "pnpm build && pnpm test:types && pnpm lint",
    "release": "dotenv release-it",
    "test:types": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.8.0",
    "defu": "^6.1.4",
    "eslint": "9.14.0",
    "eslint-plugin-format": "0.1.2",
    "prettier": "3.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "dotenv-cli": "^7.4.2",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.3",
    "release-it": "^17.10.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
