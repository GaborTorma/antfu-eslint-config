{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@8.15.3",
  "description": "Default options for @antfu/eslint-config",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "nr dev && eslint .",
    "lint:fix": "nr dev && eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --access=public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "view": "nr dev && eslint-flat-config-viewer",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "defu": "^6.1.4",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.11.28",
    "bumpp": "^9.4.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.0",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
