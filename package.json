{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "0.4.3",
  "packageManager": "pnpm@8.15.5",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "nr stub && eslint .",
    "lint:fix": "nr stub && eslint . --fix",
    "release": "bumpp && npm publish --access=public",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "2.11.3",
    "@nuxt/eslint": "0.3.0-beta.5",
    "defu": "^6.1.4",
    "eslint": "8.57.0",
    "eslint-plugin-format": "0.1.0",
    "prettier": "3.2.5",
    "vite-plugin-eslint2": "4.4.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.11.30",
    "bumpp": "^9.4.0",
    "eslint-flat-config-viewer": "^0.1.14",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
