{
  "name": "@gabortorma/antfu-eslint-config",
  "type": "module",
  "version": "1.0.0-beta.7",
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf",
  "description": "Default options for @antfu/eslint-config",
  "author": "GÃ¡bor Torma <gabor@torma.dev>",
  "license": "MIT",
  "homepage": "https://github.com/gabortorma/antfu-eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabortorma/antfu-eslint-config.git"
  },
  "bugs": "https://github.com/gabortorma/antfu-eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & npx @eslint/config-inspector",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stub:lint": "nr stub && eslint .",
    "stub:lint:fix": "nr stub && eslint . --fix",
    "release": "bumpp && npm publish --access=public",
    "postrelease": "pnpm update \"@gabortorma/antfu-eslint-config\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/eslint-config": "2.25.1",
    "@nuxt/eslint": "0.5.0",
    "defu": "^6.1.4",
    "eslint": "9.9.0",
    "eslint-plugin-format": "0.1.2",
    "prettier": "3.3.3",
    "vite-plugin-eslint2": "4.4.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.22.1",
    "@eslint/config-inspector": "^0.5.2",
    "@types/node": "^20.14.15",
    "bumpp": "^9.5.1",
    "lint-staged": "^15.2.8",
    "pnpm": "^9.7.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
